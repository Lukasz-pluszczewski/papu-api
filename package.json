{
  "name": "lowdb-rest-boilerplate",
  "version": "1.0.0",
  "description": "lowdb-rest-boilerplate",
  "repository": "https://github.com/Lukasz-pluszczewski/lowdb-rest-boilerplate",
  "main": "dist",
  "scripts": {
    "dev": "NODE_PATH=src ENV=development nodemon -w ./ --ignore \"db\" --exec \"babel-node src\"",
    "build": "NODE_PATH=src babel src -s -D -d dist",
    "start": "NODE_PATH=dist ENV=production node dist",
    "prestart": "NODE_PATH=src npm run -s build",
    "lint": "NODE_PATH=src eslint src || true",
    "lint:fix": "NODE_PATH=src eslint src --fix",
    "fixtures:users": "NODE_PATH=src babel-node fixtures/users.js --presets env,stage-0",
    "generateToken": "NODE_PATH=src babel-node fixtures/generateToken.js --presets env,stage-0"
  },
  "author": "≈Åukasz Pluszczewski",
  "license": "MIT",
  "dependencies": {
    "all-log": "1.1.0",
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "compression": "1.7.3",
    "cors": "2.8.5",
    "express": "4.16.4",
    "express-winston": "2.4.0",
    "http-proxy": "1.16.2",
    "jsonwebtoken": "7.2.1",
    "lodash": "4.17.11",
    "lodash-id": "0.14.0",
    "lowdb": "1.0.0",
    "node-cmd": "3.0.0",
    "password-hash": "1.2.2",
    "pm2": "2.7.1",
    "request": "2.83.0",
    "rimraf": "2.6.2",
    "simple-git": "1.79.0",
    "socket.io": "2.0.3",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-env": "1.6.0",
    "babel-preset-stage-0": "6.24.1",
    "eslint": "4.8.0",
    "eslint-config-brainhub": "1.6.3",
    "nodemon": "1.12.1"
  }
}
