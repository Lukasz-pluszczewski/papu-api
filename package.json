    "cors": "2.8.5",
    "express": "4.16.4",
    "express-winston": "2.4.0",
    "http-proxy": "1.16.2",
    "jsonwebtoken": "7.2.1",
    "lodash": "4.17.11",
    "lodash-id": "0.14.0",
    "lowdb": "1.0.0",
    "mongodb": "3.1.10",
    "node-cmd": "3.0.0",
    "password-hash": "1.2.2",
